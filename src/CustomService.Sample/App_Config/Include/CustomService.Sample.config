<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <services>
      <configurator type= "CustomService.DependencyInjection.CustomServiceConfigurator, CustomService.Sample"/>
    </services>

    <!-- Fixes for Warnings in sitecore logs where invalid values are specified in sitecore.config -->
    <setting name="Media.LowercaseUrls" value="false" />
    <setting name="Media.AlwaysIncludeServerUrl" value="false" />

  </sitecore>
</configuration>